#include <LiquidCrystal.h>
int RS = 6;
int EN = 7;
int DB4 = 8;
int DB5 = 9;
int DB6 = 10;
int DB7 = 11;

LiquidCrystal lcd(RS, EN, DB4, DB5, DB6, DB7);
byte dude[8] {
  0b01110, 0b11111, 0b01110, 0b00100, 0b11111, 0b00100, 0b01110, 0b01010
};

lcd.createChar(0, dude);
void setup() {
  lcd.begin(16, 2);
  lcd.clear();
  lcd.setCursor(0, 0);
}

void loop() {
  int i = 0;
  for (i; i<10; i++) {
    lcd.setCursor(i, 0);
    lcd.print(cjhar(0));
    delay(1000);
    lcd.clear();
  }
}
